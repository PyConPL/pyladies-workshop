try:
    waluty=("PLN", "USD")
    waluty.remove("PLN")
except AttributeError:
    print("nie można używać remove względem krotek!")



#zmien krotke na liste

krotka_liczbowa=(1,2,3,4,5)
pusta_lista=[]

def krotka_na_liste(krotka, lista):
    for element in krotka:
        lista.append(element)
    return lista

print(krotka_na_liste(krotka_liczbowa, pusta_lista))


jeszcze_jedna_krotka=("A", "B", "C")
def compreension_dla_krotki(tuple):
    lista_z_krotki = [element for element in tuple]
    return lista_z_krotki
print(compreension_dla_krotki(jeszcze_jedna_krotka))